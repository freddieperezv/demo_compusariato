<odoo>

    <record model="ir.ui.view" id="view_res_partner_import">
        <field name="name">res.partner.import</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='category_id']" position="after">
                <field name="international" widget="boolean_toggle"/>
            </xpath>
        </field>
    </record>
  
</odoo>