<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <!-- Creating demo data that require looking for accounts -->
        <record id="account_asset_mueble_enseres" model="account.asset">
            <field name="name">Muebles y Enseres</field>
            <field name="method">linear</field>
            <field name="method_number">60</field>
            <field name="method_period">1</field>
            <field name="account_asset_id" model="account.account" search="[('code', '=', '120105')]"/>
            <field name="account_depreciation_id" model="account.account" search="[('code', '=', '12011204')]"/>
            <field name="account_depreciation_expense_id" model="account.account" search="[('code', '=', '5201210104')]"/>
            <field name="journal_id" model="account.journal" search="[('name', '=', 'Operaciones varias')]"/>
            <!-- <field name="account_analytic_id" model="" search="[('name', '=', 'Operaciones varias')]"/> -->
            <field name="asset_type">purchase</field>
            <field name="state">model</field>
        </record>

